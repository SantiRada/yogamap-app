{
  "expo": {
    "name": "yogamap",
    "slug": "yogamap",
    "privacy": "public",
    "jsEngine": "hermes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "permissions": [
      "MEDIA_LIBRARY"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A122E"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1A122E"
      }
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    }
  }
}
